{
  "name": "desbordante",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "download-schema": "apollo service:download --endpoint=http://localhost:5000/graphql --header=\"X-Device: \" --header=\"X-Request-Id: test\"",
    "codegen": "apollo codegen:generate --globalTypesFile=src/types/globalTypes.ts --localSchemaFile=./schema.json --target=typescript --tagName=gql",
    "codegen:server": "apollo codegen:generate --globalTypesFile=src/types/globalTypes.ts --localSchemaFile=../server/schema.json --target=typescript --tagName=gql",
    "codegen:cms": "apollo codegen:generate --globalTypesFile=src/types/cmsTypes.ts --localSchemaFile=../cms/schema.graphql --target=typescript --tagName=cmsGql",
    "dev": "next dev",
    "dev:local": "NEXT_PUBLIC_TYPE=local next dev",
    "dev:prod": "NEXT_PUBLIC_TYPE=prod next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --coverage --verbose --runInBand",
    "test:coverage": "jest --coverage --maxWorkers=50%"
  },
